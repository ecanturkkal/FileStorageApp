{
  "ConnectionStrings": {
    "DbConnection": "{your_db_connection}",
    "AzureBlobConnection": "{your_azure_blob_connection}"
  },
  "AzureBlobStorage": {
    "ContainerName": "{your_container}"
  },
  "Jwt": {
    "Key": "{your_key}",
    "Issuer": "{your_issuer}",
    "Audience": "{your_audience}",
    "ExpirationInMinutes": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
